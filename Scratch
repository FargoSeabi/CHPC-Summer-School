movies =["movie1.mp4","movie2.mp4"]

file =open("movie3.mp4","w")
file.write("")
file.close()

with open("movie4.mp4","w") as file:
    file.write("")
    
for file in movies:
    with open(file,"w") as file:
        file.writable()
    
